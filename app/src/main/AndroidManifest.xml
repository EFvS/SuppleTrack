<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.efvs.suppletrack">

    <uses-permission android:name="android.permission.SCHEDULE_EXACT_ALARM"/>
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS"/>

    <application
        android:name=".SuppleTrackApp"
        android:label="@string/app_name"
        android:theme="@style/Theme.SuppleTrack">
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <receiver android:name=".MissedDoseReceiver" android:exported="false"/>
        <receiver android:name=".DoseTakenReceiver" android:exported="false"/>
        <receiver android:name=".MinuteCheckReceiver" android:exported="false"/>
    </application>
</manifest>